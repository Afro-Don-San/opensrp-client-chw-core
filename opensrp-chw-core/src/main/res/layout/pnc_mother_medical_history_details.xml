<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linearLayoutMotherPncHealthFacilityVisit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="visible">

            <org.smartregister.view.customcontrols.CustomFontTextView
                android:id="@+id/customFontTextViewPncHealthFacilityVisitMotherTitle"
                style="@style/CustomFontTextViewStyle.ClientList"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/medical_history"
                android:background="@color/medical_bar"
                android:paddingLeft="20dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:text="@string/pnc_medical_history_mother_title"
                android:textColor="@color/white"
                android:textSize="14sp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayoutPncHealthFacilityVisit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="visible">

            <org.smartregister.view.customcontrols.CustomFontTextView
                android:id="@+id/customFontTextViewPncHealthFacilityVisitTitle"
                style="@style/CustomFontTextViewStyle.ClientList"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/linearLayoutMotherPncHealthFacilityVisit"
                android:paddingLeft="20dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:text="@string/pnc_health_facility_visits_title"
                android:textColor="@color/medical_text_inner"
                android:textSize="14sp" />


            <LinearLayout
                android:id="@+id/linearLayoutPncHealthFacilityVisitDetails"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="20dp">

            </LinearLayout>

        </LinearLayout>

        <View
            android:id="@+id/viewHealthFacilityVisitsRow"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_below="@+id/linearLayoutPncHealthFacilityVisit"
            android:background="@color/light_grey"
            android:visibility="visible" />

        <LinearLayout
            android:id="@+id/linearLayoutPncFamilyPlanning"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="visible">

            <org.smartregister.view.customcontrols.CustomFontTextView
                android:id="@+id/customFontTextViewPncFamilyPlanningTitle"
                style="@style/CustomFontTextViewStyle.ClientList"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/linearLayoutMotherPncHealthFacilityVisit"
                android:paddingLeft="20dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:text="@string/pnc_medical_history_family_planning_title"
                android:textColor="@color/medical_text_inner"
                android:textSize="14sp" />

            <LinearLayout
                android:id="@+id/linearLayoutPncFamilyPlanningDetails"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="20dp">

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>
</LinearLayout>